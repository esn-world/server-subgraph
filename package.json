{
  "name": "@tumi/server",
  "version": "1.0.0",
  "main": "src/server.ts",
  "license": "MIT",
  "scripts": {
    "dev": "dotenv -e .env.development -- ts-node-dev --no-notify --respawn src/server",
    "test": "yarn prisma:reset-test && dotenv -e .env.test -- ts-node-dev --no-notify --respawn --transpile-only src/server",
    "dev:full-db": "ts-node-dev --respawn --transpile-only  src/server",
    "start": "npm run build:production && yarn copy && node ./dist/server.js",
    "clean": "rimraf dist",
    "copy": "copyfiles -u 2 src/generated/prisma/** dist/generated && copyfiles -u 1 src/assets/** dist",
    "build": "yarn clean && yarn generate && tsc && yarn copy",
    "build:production": "yarn clean && yarn generate && tsc -p tsconfig.production.json && yarn copy",
    "generate": "yarn generate:prisma",
    "generate:prisma": "prisma generate",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "prisma:seed-dev": "dotenv -e .env.development -- ts-node prisma/seed.ts",
    "prisma:studio-dev": "dotenv -e .env.development -- prisma studio",
    "prisma:reset-dev": "dotenv -e .env.development -- yarn prisma db push --accept-data-loss",
    "prisma:reset-test": "dotenv -e .env.test -- yarn prisma migrate reset --skip-seed --force && dotenv -e .env.test -- yarn prisma db push --accept-data-loss && dotenv -e .env.test -- yarn prisma db seed",
    "prisma:seed-test": "dotenv -e .env.test -- yarn prisma db seed",
    "test-old": "yarn docker:up && dotenv -e .env.test -- yarn prisma migrate reset --force && dotenv -e .env.test -- jest -i --detectOpenHandles",
    "prepare-test": "yarn docker:up && dotenv -e .env.test -- yarn prisma migrate reset --force"
  },
  "dependencies": {
    "@apollo/server": "^4.1.1",
    "@apollo/subgraph": "^2.2.1",
    "@azure/cognitiveservices-computervision": "^8.2.0",
    "@azure/storage-blob": "^12.12.0",
    "@pothos/core": "^3.23.0",
    "@pothos/plugin-directives": "^3.9.0",
    "@pothos/plugin-federation": "^3.7.0",
    "@pothos/plugin-prisma": "^3.36.0",
    "@pothos/plugin-scope-auth": "3.16.0",
    "@pothos/plugin-simple-objects": "^3.6.7",
    "@pothos/plugin-tracing": "^0.5.7",
    "@pothos/tracing-sentry": "^0.7.8",
    "@prisma/client": "^4.6.1",
    "@sentry/integrations": "^7.20.1",
    "@sentry/node": "^7.20.1",
    "@sentry/tracing": "^7.20.1",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-jwt": "^7.7.7",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^3.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.1.0",
    "marked": "^4.2.3",
    "mjml": "^4.13.0",
    "redis": "^4.5.0",
    "sharp": "^0.31.2",
    "stripe": "^10.17.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@swc/core": "^1.3.19",
    "@swc/helpers": "^0.4.14",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-jwt": "^7.4.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.189",
    "@types/luxon": "^3.1.0",
    "@types/marked": "^4.0.7",
    "@types/mjml": "^4.7.0",
    "@types/node": "18.11.9",
    "@types/sharp": "^0.31.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "fs-jetpack": "^5.1.0",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "prisma": "^4.6.1",
    "prisma-dbml-generator": "^0.9.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "~4.9.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "volta": {
    "node": "19.1.0"
  }
}
